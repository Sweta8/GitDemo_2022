package sorting;

public class QuickSort {
	
	public static void main(String[] args)
	{
		int a[]= {2,1,6,0,3,2,7,9,0,3};
		int temp;
		boolean flag=false;
		for(int i=0;i<a.length;i++)
		{
			for(int j=0;j<a.length-1-i;j++)
			{
				if(a[j]>a[j+1]) {
				temp=a[j];
				a[j]=a[j+1];
				a[j+1]=temp;
				flag=true;
			}
			}
			if(flag==false)
				break;
			else 
				flag=false;
			
		}
		
		for(int i=0;i<a.length;i++)
		{
			System.out.print(a[i]+" ");
		}
	}

}
