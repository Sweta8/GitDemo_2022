package sorting;

public class Test {
	
	public static void main(String[] args) {
		
		int a[]= {1,4,2,3,8,5,7,1,0};
		int min;
		int temp;
		int key,j;
		for(int i=1;i<a.length;i++)
		{
			key=a[i];
			j=i-1;
			
			while(j>=0 && a[j]>a[j+1])
			{
				a[j+1]=a[j];
			}
			a[j+1]=key;
		}
		
		for(int i=0;i<a.length;i++)
		{
			System.out.print(a[i]+" ");
		}
		
	}

}
