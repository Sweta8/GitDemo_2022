package twilio;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class HowManySentences {
	
	
	public static int binarySerach(int a[],int low,int high,int value)
	{
		int mid;
		while(low<=high)
		{
			mid=(low+high)/2;
			if(a[mid]==value)
			{
				return mid;
			}
			else if(a[mid]<value)
			{
				binarySerach(a, mid+1, high, value);
			}
			else
			{
				binarySerach(a, low, mid-1, value);
			}
		}
		return -1;
		
	}
	
	public static void main(String [] args)
	{
		
			
	}
		
		

}
