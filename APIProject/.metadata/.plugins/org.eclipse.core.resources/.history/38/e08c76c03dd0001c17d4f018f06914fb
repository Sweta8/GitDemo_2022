package linkedList;

public class Practice {
	
	public static void main(String[] args)
	{
		int a[]= {1,0,0,0,1,1,1,0,0};
		
	}
	
	public int countNoOfNodes(int a[],int n)
	{
		int low=0;
		int high=a.length-1;
		while(low<high)
		{
			mid=(low+high)/2;
			if(a[low]>1)
			{
				low=mid+1;
			}
			else if(a[low]<1)
			{
				
			}
		}
	}

}
