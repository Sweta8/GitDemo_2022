package sorting;

public class Test {
	
	public static void main(String[] args) {
		
		int a[]= {1,3,1,0,9};
		int temp;
		boolean flag=false;
		
		for(int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length-1-i;j++)
			{
				if(a[j]>a[j+1])
				{
					temp=a[j];
					a[j]=a[j+1];
					a[j]=temp;
					flag=true;
				}
			}
		}
		
		
	}

}
