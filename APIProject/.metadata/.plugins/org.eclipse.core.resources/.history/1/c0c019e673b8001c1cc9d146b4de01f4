package sorting;

public class Test {
	
	public static void main(String[] args) {
		
		int x[]= {1,4,2,3,8,5,7,1,0};
		int temp;
		boolean flag=false;
		for(int i=0;i<x.length;i++)
		{
			for(int j=i+1;j<x.length-1-i;j++)
			{
				if(x[j]>x[j+1])
				{
					temp=x[j];
					x[j]=x[j+1];
					x[j+1]=temp;
					flag=true;
				}
			}
			if(flag==false)
				break;
			else
				flag=false;
		}
		
		for(int i=0;i<x.length;i++)
		{
			System.out.print(x[i]+" ");
		}
		
		
	}

}
