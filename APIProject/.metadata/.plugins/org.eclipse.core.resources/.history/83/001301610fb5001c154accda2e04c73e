package sorting;

public class QuickSort {
	
	public static void main(String[] args)
	{
		int a[]= {1,3,6,0,9,3};
		int key;
		int j;
		for(int i=1;i<a.length;i++)
		{
			key=a[i];
			j=i-1;
			
			while(j>=0 && a[j]>key)
			{
				a[j+1]=a[j];
				j--;
			}
			
			a[j+1]=key;
		}
		
		for(int i=0;i<a.length;i++)
		{
			System.out.print(a[i]+" ");
		}
	}

}
