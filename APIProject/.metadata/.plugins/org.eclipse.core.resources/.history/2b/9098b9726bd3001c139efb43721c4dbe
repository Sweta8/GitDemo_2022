package checkingAndTesting;

public class Practice {
	
	public static int binarySearch(int a[],int low,int high,int key)
	{
		int mid=(low+high)/2;
		
		if(a[mid]==key)
		{
			return mid;
		}
		else if(a[mid]<key)
		{
			return binarySearch(a, low, mid-1, key);
		}
		else if(a[mid]>key)
		{
			return binarySearch(a, mid+1, high, key);
		}
		
		retrun -1;
		
	}

}
