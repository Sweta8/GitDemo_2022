package twilio;

import java.util.LinkedHashMap;
import java.util.Map;

public class HowManySentences {
	
	public static void main(String[] args)
	{
		int a=678;
		int sum=0;
		int d;
		while(a>0 || sum>9)
		{
			if(a==0)
			{
				a=sum;
				sum=0;
			}
			d=a%10;
			sum=sum+d;
			a=a/10;
		
		}
		
		System.out.println(sum);
		
	}
}
