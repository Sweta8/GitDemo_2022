package sorting;

public class QuickSort {
	
	public static void main(String[] args)
	{
		
		int a[]= {3,1,3,2,5,6,0,0};
		int min;
		int temp;
		
		for(int i=0;i<a.length-1;i++)
		{
			min=i;
			
			for(int j=0;j<a.length;j++)
			{
				if(a[j]<a[min])
				{
					min=j;
				}
			}
			
			
		}
		
		
		
	}

}
