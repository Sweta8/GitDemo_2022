package linkedList;

public class Runner {
	
	public static void main(String[] args)
	{
		LinkedList l=new LinkedList();
		l.insert(20);
		l.insert(30);
		l.insert(50);
		l.insert(0);
		l.insert(40);
		System.out.print("1st Insertion: ");
		l.show();
		int c=l.show();
		System.out.println("No of nodes: "+c);
		int middle=c/2;
		int value=l.middleElement(middle);
		System.out.println("Middle Element Value is ");
	}

}
