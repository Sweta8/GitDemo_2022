package linkedList;

public class Practice {
	
	public static void main(String[] args)
	{
		
		int a[]= { 1, 3, 20, 4, 1, 0 };
		int n=a.length;
		if(a.length==0)
			System.out.println("No values");
		if(a[n-1]>=a[n-2])
			System.out.println(a[n]);
		if(a[0]>a[1])
			System.out.println(a[n]);
		
	}

}
